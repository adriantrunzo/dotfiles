{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "javascript",
    "javascriptreact",
    "json",
    "markdown",
    "scss",
    "typescript",
    "typescriptreact"
  ],
  "coc.preferences.formatOnType": true,
  "list.source.files.args": ["--hidden", "--files"],
  "prettier.eslintIntegration": true,
  "prettier.requireConfig": true,
  "eslint.autoFixOnSave": true
}
