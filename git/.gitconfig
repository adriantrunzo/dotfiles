[core]
  editor = vim
  pager = delta

[delta]
  navigate = true

[diff]
  colorMoved = default

[interactive]
  diffFilter = delta --color-only

[merge]
  conflictstyle = diff3

[pull]
  ff = only

[push]
  autoSetupRemote = true

[submodule]
  recurse = true

[include]
  path = ./.gitconfig-local
